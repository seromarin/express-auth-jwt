{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "object-curly-spacing": ["error", "always", { "objectsInObjects": false }],
        "max-len": ["error", { "ignoreStrings": true }]
    }
}